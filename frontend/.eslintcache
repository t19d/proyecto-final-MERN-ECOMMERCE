tidadStock = 0;\n        switch (talla) {\n            case \"XS\":\n                cantidadStock = data.cantidadStockXS;\n                break;\n            case \"S\":\n                cantidadStock = data.cantidadStockS;\n                break;\n            case \"M\":\n                cantidadStock = data.cantidadStockM;\n                break;\n            case \"L\":\n                cantidadStock = data.cantidadStockL;\n                break;\n            case \"XL\":\n                cantidadStock = data.cantidadStockXL;\n                break;\n            case \"-\":\n                cantidadStock = data.cantidadStockXS;\n                break;\n            default:\n                cantidadStock = data.cantidadStockXS;\n                break;\n        }\n        dispatch({\n            type: ANHADIR_ITEM_AL_CARRITO,\n            payload: {\n                producto: data._id,\n                nombre: data.nombre,\n                miniatura: data.miniatura,\n                precio: data.precioOferta,\n                cantidadStock: cantidadStock,\n                cantidad: cantidad,\n                talla: talla\n            }\n        });\n        const { carrito: { carritoItems } } = getState();\n        /* COOKIES */\n        //Cookie.set(\"carritoItems\", JSON.stringify(carritoItems));\n        /* localstorage */\n        localStorage.setItem('carritoItems', JSON.stringify(carritoItems));\n    } catch (error) {\n    }\n}\n\nconst eliminarDelCarrito = (productoId) => (dispatch, getState) => {\n    dispatch({ type: ELIMINAR_ITEM_DEL_CARRITO, payload: productoId });\n\n    const { carrito: { carritoItems } } = getState();\n    /* COOKIES */\n    //Cookie.set(\"carritoItems\", JSON.stringify(carritoItems));\n    /* localstorage */\n    localStorage.setItem('carritoItems', JSON.stringify(carritoItems));\n}\n\nconst guardarEnvio = (data) => (dispatch) => {\n    dispatch({ type: CARRITO_GUARDAR_ENVIO, payload: data });\n    /* localstorage */\n    localStorage.setItem('envio', JSON.stringify(data));\n}\n\nconst guardarPago = (data) => (dispatch) => {\n    dispatch({ type: CARRITO_GUARDAR_PAGO, payload: data });\n}\n\nexport { anhadirAlCarrito, eliminarDelCarrito, guardarEnvio, guardarPago }","D:\\Documents\\proyecto-final-DAW\\proyecto-final MERN ECOMMERCE\\proyecto-final-MERN-ECOMMERCE\\frontend\\src\\reducers\\usuarioReducers.js",[],"D:\\Documents\\proyecto-final-DAW\\proyecto-final MERN ECOMMERCE\\proyecto-final-MERN-ECOMMERCE\\frontend\\src\\constants\\usuarioConstantes.js",[],"D:\\Documents\\proyecto-final-DAW\\proyecto-final MERN ECOMMERCE\\proyecto-final-MERN-ECOMMERCE\\frontend\\src\\screens\\InicioSesionScreen.js",[],"D:\\Documents\\proyecto-final-DAW\\proyecto-final MERN ECOMMERCE\\proyecto-final-MERN-ECOMMERCE\\frontend\\src\\actions\\usuarioAcciones.js",[],"D:\\Documents\\proyecto-final-DAW\\proyecto-final MERN ECOMMERCE\\proyecto-final-MERN-ECOMMERCE\\frontend\\src\\screens\\RegistroScreen.js",["192"],"import React, { useEffect, useState } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { Link } from 'react-router-dom';\nimport { registrar } from '../actions/usuarioAcciones';\n\nfunction RegistroScreen(props) {\n    const [nombre, setNombre] = useState('');\n    const [email, setEmail] = useState('');\n    const [password, setPassword] = useState('');\n    const [rePassword, setRePassword] = useState('');\n    const usuarioRegistro = useSelector(state => state.usuarioRegistro);\n    const { loading, usuarioInfo, error } = usuarioRegistro;\n    const dispatch = useDispatch();\n\n    const redirect = props.location.search ? props.location.search.split(\"=\")[1] : \"/\";\n\n    const submitHandler = (e) => {\n        e.preventDefault();\n\n        if (password !== rePassword) {\n            alert('Las contraseñas no coinciden.');\n        } else {\n            dispatch(registrar(nombre, email, password));\n        }\n\n    };\n\n    useEffect(() => {\n        if (usuarioInfo) {\n            props.history.push(redirect);\n        }\n        return () => {\n            //\n        };\n    }, [usuarioInfo]);\n\n    return (\n        <div className=\"container-sm\" onSubmit={submitHandler}>\n            <h1 className=\"text-center tituloPagina\">Registrarse</h1>\n            {loading && <div className=\"d-flex justify-content-center\"><img src=\"/images/recursos_web/loading.gif\" alt=\"Cargando\" /></div>}\n            {error && <div className=\"text-danger text-center\">{error}</div>}\n            <form className=\"formRegistro\">\n                <label forhtml=\"inputNombre\" className=\"labelFormularioUsuario\">Nombre:</label>\n                <input id=\"inputNombre\" className=\"form-control inputFormularioUsuario\" type=\"text\" name=\"name\" placeholder=\"Nombre\" required autoFocus onChange={(e) => setNombre(e.target.value)} />\n                <label forhtml=\"inputEmail\" className=\"labelFormularioUsuario\">Email:</label>\n                <input id=\"inputEmail\" className=\"form-control inputFormularioUsuario\" type=\"email\" name=\"email\" placeholder=\"Email\" required onChange={(e) => setEmail(e.target.value)} />\n                <label forhtml=\"inputPassword\" className=\"labelFormularioUsuario\">Contraseña:</label>\n                <input id=\"inputPassword\" minLength=\"8\" className=\"form-control inputFormularioUsuario\" type=\"password\" name=\"password\" placeholder=\"Contraseña\" required onChange={(e) => setPassword(e.target.value)} />\n                <label forhtml=\"inputRePassword\" className=\"labelFormularioUsuario\">Repita la contraseña:</label>\n                <input id=\"inputRePassword\" minLength=\"8\" className=\"form-control inputFormularioUsuario\" type=\"password\" name=\"repassword\" placeholder=\"Repita la contraseña\" required onChange={(e) => setRePassword(e.target.value)} />\n                <div className=\"text-center\">\n                    <button className=\"btn botonFormularioUsuario\" type